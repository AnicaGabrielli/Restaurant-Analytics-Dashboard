{
  "name": "restaurant-analytics-dashboard",
  "version": "2.0.0",
  "description": "Sistema completo de analytics para restaurantes multi-canal",
  "type": "module",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "node --watch backend/server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint backend/**/*.js",
    "db:create": "node backend/scripts/createDatabase.js",
    "db:seed": "python database/generate_data_mysql.py",
    "db:indexes": "node backend/scripts/createIndexes.js"
  },
  "keywords": [
    "restaurant",
    "analytics",
    "dashboard",
    "mysql",
    "express",
    "nodejs"
  ],
  "author": "Restaurant Analytics Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}