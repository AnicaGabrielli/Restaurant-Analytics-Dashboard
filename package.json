{
  "name": "restaurant-analytics-dashboard",
  "version": "2.0.0",
  "description": "Sistema completo de analytics para restaurantes multi-canal",
  "type": "module",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "node --watch backend/server.js",
    "debug": "node debug-connection.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint backend/**/*.js",
    "db:create": "mysql -u root -p -e \"CREATE DATABASE IF NOT EXISTS challenge_db CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci;\"",
    "db:schema": "mysql -u root -p challenge_db < database/schema-mysql.sql",
    "db:indexes": "mysql -u root -p challenge_db < database/indexes.sql",
    "db:seed": "python3 database/generate_data_mysql.py",
    "db:setup": "npm run db:create && npm run db:schema && npm run db:indexes",
    "logs": "tail -f logs/app.log",
    "logs:error": "tail -f logs/error.log",
    "clean": "rm -rf node_modules package-lock.json logs/*.log",
    "reinstall": "npm run clean && npm install"
  },
  "keywords": [
    "restaurant",
    "analytics",
    "dashboard",
    "mysql",
    "express",
    "nodejs"
  ],
  "author": "Restaurant Analytics Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}